{
    "name" : "covex-nn/vfs",
    "type" : "library",
    "description" : "PHP stream wrapper, transactional virtual file system",
    "keywords" : [
        "wrapper",
        "testing",
        "mock file system"
    ],
    "license" : "MIT",
    "authors": [
        {
            "name" : "Andrey F. Mindubaev",
            "email" : "covex.mobile@gmail.com"
        }
    ],
    "require": {
        "php" : "^7.1",
        "symfony/filesystem": "^3.4 || ^4.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload" : {
        "psr-4" : {
            "Covex\\Stream\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Covex\\Stream\\Tests\\" : "tests/"
        }
    }
}
